[
  {
    "name": "atproto_pds",
    "image": "${image}",
    "portMappings": [
      {
        "containerPort": 80,
        "hostPort": 80
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-region": "${region}",
        "awslogs-stream-prefix": "atproto_pds",
        "awslogs-group": "${cloudwatch_group_name}"
      }
    }
  }
]
